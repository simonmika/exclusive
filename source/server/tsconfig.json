{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"strictNullChecks": false,
		"sourceMap": true,
		"sourceRoot": "../",
		"outDir": "../../build/",
		"types": [
			"node",
			"jquery",
			"jquerymobile"
		]
	},
	"exclude": [
		"node_modules"
	],
	"files": [
		"Exclusive.ts"
	]
}
