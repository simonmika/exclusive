{
	"compilerOptions": {
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"strictNullChecks": false,
		"sourceMap": true,
		"types": [
			"node",
			"jquery",
			"jquerymobile"
		]
	},
	"exclude": [
		"node_modules"
	]
}
