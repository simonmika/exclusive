{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch App",
			"type": "composite",
			"request": "launch",
			"configurationNames": [
				"Launch Chrome",
				"Launch Server"
			]
		},
		{
			"name": "Launch Chrome",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8080/",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Launch Server",
			"type": "node",
			"request": "launch",
			"preLaunchTask": "build-server",
			"program": "${workspaceRoot}/build/server/Exclusive.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
				"DEBUG": "express:*"
			},
			"console": "internalConsole",
			"sourceMaps": true
		}
	]
}
